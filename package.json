{
  "name": "vcwebnetworks-nodets",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "repository": "git@github.com:vagnercardosoweb/vcwebnetworks-nodets.git",
  "author": "Vagner Cardoso <vagnercardosoweb@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules './src/server.ts'",
    "dev:docker": "docker-compose -f docker-compose.yml up",
    "prestart": "yarn build",
    "start": "node ./dist/server.js",
    "poststart": "yarn db:migrate",
    "build": "babel ./src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "lint": "eslint --ext ts ./src",
    "lint:fix": "yarn lint --fix",
    "predb:migrate": "babel ./src/database/migrations/source --extensions \".js,.ts\" --out-dir ./src/database/migrations/compiled",
    "db:migrate": "sequelize db:migrate",
    "predb:seed": "babel ./src/database/seeders/source --extensions \".js,.ts\" --out-dir ./src/database/seeders/compiled",
    "db:seed": "sequelize db:seed:all",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@sentry/node": "^6.2.5",
    "@sentry/tracing": "^6.2.5",
    "bcryptjs": "^2.4.3",
    "celebrate": "^14.0.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.5.0",
    "http-graceful-shutdown": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.33",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/module-alias": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.41",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
